{
  "name": "jbhm",
  "version": "2.0.1",
  "description": "Custom Theme for JBHM Architecture built by Creative Distillery.",
  "scripts": {
    "start": "parcel watch new-assets/index.js new-assets/main.scss -d dist",
    "build": "parcel build new-assets/index.js new-assets/main.scss -d dist",
    "start-timeline": "parcel watch blocks/timeline/src/timeline.js blocks/timeline/src/timeline-styles.scss -d blocks/timeline/dist",
    "build-timeline": "parcel build blocks/timeline/src/timeline.js blocks/timeline/src/timeline-styles.scss -d blocks/timeline/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willfalcon/jbhm-theme.git"
  },
  "author": "Creative Distillery",
  "private": true,
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "sass": "^1.20.1"
  },
  "dependencies": {
    "flickity": "^2.2.0",
    "flickity-imagesloaded": "^2.0.0"
  }
}
